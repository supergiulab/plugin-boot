    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@fancyapps/fancybox": "^3.5.7",
    "glob-parent": "^6.0.2",
    "masonry-layout": "^4.2.2",
    "npm": "^8.12.1",
    "owl.carousel": "^2.3.4",
    "popper.js": "^1.16.1",
